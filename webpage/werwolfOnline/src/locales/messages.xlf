      <trans-unit id="1388948412142700964" datatype="html">
        <source>Wie wird gespielt?</source>
        <context-group purpose="location">
          <context context-type="sourcefile">src/app/how-to-play/how-to-play.component.html</context>
          <context context-type="linenumber">4</context>
        </context-group>
